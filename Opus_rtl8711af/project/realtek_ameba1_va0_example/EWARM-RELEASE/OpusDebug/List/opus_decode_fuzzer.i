typedef struct {
    int fs;
    int channels;
} TocInfo;

static void ParseToc(const uint8_t *toc, TocInfo *const info) {
    const int samp_freqs[5] = {8000, 12000, 16000, 24000, 48000};
    const int bandwidth = opus_packet_get_bandwidth(toc);

    info->fs = samp_freqs[bandwidth - 1101];
    info->channels = opus_packet_get_nb_channels(toc);
}






 
int LLVMFuzzerTestOneInput(const uint8_t *data, size_t size) {
    OpusDecoder *dec;
    opus_int16 *pcm;
    uint8_t *packet;
    TocInfo toc;
    int i, err;

     
    if (size < 8 + 1) {
        return 0;
    }

     
    ParseToc(&data[8], &toc);

    dec = opus_decoder_create(toc.fs, toc.channels, &err);
    if (err != 0 | dec == 0) {
        return 0;
    }

    pcm = (opus_int16*) malloc(sizeof(*pcm) * 5760 * toc.channels);
    packet = (uint8_t*) calloc(1500, sizeof(*packet));

    i = 0;
    while (1) {
        int len, fec;

        if (i + 8 >= size) {
            break;
        }

        len = (opus_uint32) data[i    ] << 24 |
              (opus_uint32) data[i + 1] << 16 |
              (opus_uint32) data[i + 2] <<  8 |
              (opus_uint32) data[i + 3];
        if (len > 1500 || len < 0) {
            break;
        }

        
 
        fec = data[i + 4] & 1;

         
        if (len == 0) {
            int frame_size;
            opus_decoder_ctl(dec, 4039, ((&frame_size) + ((&frame_size) - (opus_int32*)(&frame_size))));
            (void) opus_decode(dec, 0, size, pcm, frame_size, fec);
        } else {
            if (i + 8 + len > size) {
                break;
            }
            memcpy(pcm, &data[i + 8], len);
            (void) opus_decode(dec, data, size, pcm, 5760, fec);
        }

        i += 8 + len;
    }

    opus_decoder_destroy(dec);
    free(pcm);
    free(packet);

    return 0;
}
